{
    "name": "studiomado/query-bundle",
    "type": "symfony-bundle",
    "authors": [
        {
            "name": "Simone Gentili",
            "email": "sensorario@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "": ["src", "tests"]
        }
    },
    "config": {
        "platform": {
            "php": "7.0.9"
        },
        "bin-dir": "bin"
    },
    "require": {
        "symfony/dependency-injection": "^3.3",
        "symfony/config": "^3.3",
        "symfony/http-kernel": "^3.3",
        "doctrine/orm": "^2.5",
        "pagerfanta/pagerfanta": "^1.0",
        "doctrine/annotations": "v1.4.0",
        "doctrine/common": "v2.7.3",
        "doctrine/dbal": "v2.5.13",
        "symfony/console": "~3.4.3",
        "symfony/debug": "~3.4.3",
        "symfony/event-dispatcher": "~3.4.3",
        "symfony/filesystem": "~3.4.3",
        "symfony/http-foundation": "~3.4.3",
        "doctrine/cache": "v1.6.2",
        "doctrine/collections": "v1.4.0",
        "doctrine/instantiator": "1.0.5",
        "willdurand/hateoas": "2.10"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.3"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.4-dev"
        }
    }
}
